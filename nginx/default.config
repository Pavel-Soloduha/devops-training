server {
	listen 80;
	location / {
		proxy_pass http://3.133.131.230:5000;
	}
}
server {
	listen              443 ssl;
	server_name         3.133.131.230;
    ssl_certificate     certs/cert.crt;
    ssl_certificate_key certs/cert.key;
    ssl_protocols       TLSv1 TLSv1.1 TLSv1.2;
    ssl_ciphers         HIGH:!aNULL:!MD5;
    location / {
        proxy_pass http://3.133.131.230:5000;
    }
}
